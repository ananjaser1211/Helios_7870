on property:ro.crypto.type=file
    pdp c_ready

on property:ro.crypto.type=block && property:vold.decrypt=trigger_load_persist_props
    pdp c_ready

# on property:ro.crypto.type=block && property:vold.decrypt=trigger_restart_framework

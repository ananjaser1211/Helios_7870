/dts-v1/;
#include "exynos7870-gtactive2_common.dtsi"
#include "modem-ss315ap-pdata.dtsi"
#include "exynos7870-gtactive2lte_eur_open_gpio_03.dtsi"
#include "z_batterydata_gtactive2lte_eur_open_common.dtsi"
#include "exynos7870-gtactive2lte_fingerprint-sensor_02.dtsi"

/ {
	model = "Samsung Galaxy Tab Active2 LTE rev03 board based on Exynos7870";
        model_info-chip = <7870>;
        model_info-platform = "android";
        model_info-subtype = "samsung";
        model_info-hw_rev = <3>;
        model_info-hw_rev_end = <3>;
	compatible = "samsung, Galaxy Tab Active2 LTE 03", "samsung,exynos7870";

	cnss_dcdc_en: cnss_dcdc_en {
		compatible = "regulator-fixed";
		regulator-name = "cnss_dcdc_en";
		startup-delay-us = <4000>;
		enable-active-high;
		gpio = <&gpf3 0 0>;
	};

	serial_2: uart@13820000 {
		samsung,alive-io;
	};

	pinctrl@139F0000 {
		acc_int: acc_int {
			samsung,pins = "gpa1-2";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		grip_int: grip_int {
			samsung,pins = "gpa2-4";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		grip_wifi_int: grip_wifi_int {
			samsung,pins = "gpa2-3";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		prox_int: prox_int {
			samsung,pins = "gpa1-4";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		/delete-node/ bt-btwake;
	};

	i2c@13860000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&grip_i2c>;
		
		sx9306-i2c@28 {
			compatible = "sx9306-i2c";
			reg = <0x28>;
			pinctrl-names = "default";
			pinctrl-0 = <&grip_int>;
			interrupt-parent = <&gpa2>;
			interrupts = <4 0 0>;
			sx9306-i2c,nirq-gpio = <&gpa2 4 0>;
			#sx9306-i2c,ctrl0 = <0x14>; /* Scanperiod : 60 */
			sx9306-i2c,ctrl2 = <0x77>; /* Gain: 8, Freq : 167, Resolution : Finest */
			sx9306-i2c,ctrl3 = <0x03>; /* Rawfilter: High */
			#sx9306-i2c,ctrl5 = <0x0f>; /* AVGNEG:001 AVGPOS : 111 */
			sx9306-i2c,normal-thd = <7>; /* normal touch threshold */
			sx9306-i2c,ch-main = <2>; /* channel number */
			sx9306-i2c,ch-sub1 = <0xff>; /*oxff: Not be used sub channel */
			sx9306-i2c,ta-thd = <0x11>; /* TA threhold : 400 */
		};
		
		sx9306_wifi-i2c@2a{
			compatible = "sx9306_wifi-i2c";
			reg = <0x2a>;
			pinctrl-names = "default";
			pinctrl-0 = <&grip_wifi_int>;
			interrupt-parent = <&gpa2>;
			interrupts = <3 0 0>;
			sx9306_wifi-i2c,nirq-gpio = <&gpa2 3 0>;
			#sx9306_wifi-i2c,ctrl0 = <0x04>; /* Scanperiod : 30 */
			sx9306_wifi-i2c,ctrl2 = <0x77>; /* Gain:8, Freq : 167, Resolution : Finest */
			sx9306_wifi-i2c,ctrl3 = <0x03>; /* Rawfilter: High */
			#sx9306_wifi-i2c,ctrl5 = <0x0f>; /* AVGNEG:001 AVGPOS : 111 */
			sx9306_wifi-i2c,normal-thd = <4>; /* normal touch threshold */
			sx9306_wifi-i2c,ch-main = <2>; /* channel number */
			sx9306_wifi-i2c,ch-sub1 = <0xff>; /*oxff: Not be used sub channel */
			sx9306_wifi-i2c,ta-thd = <0x0f>; /* TA threhold : 300 */
		};
	};
	
	i2c@138D0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&sensor_i2c &sensor_ldo_en>;

		lsm6ds3@6b {
			compatible = "st,lsm6ds3";
			reg = <0x6b>;
			pinctrl-names = "default";
			pinctrl-0 = <&acc_int>;
			interrupt-parent = <&gpa1>;
			interrupts = <2 0 0>;
			st,orientation = <1 2 1 2 1 1 1 1 0>;
			st,drdy-int-pin = <1>;
			st,irq_gpio = <&gpa1 2 0x00>;
		};

		ak09916c-i2c@0C {
			compatible = "ak09916c-i2c";
			reg = <0x0c>;
			ak09916c-i2c,chip_pos = <0>;
		};

		tmd3725@39 {
			compatible = "taos,tmd3725";
			reg = <0x39>;
			pinctrl-names = "default";
			pinctrl-0 = <&prox_int &proxy_led_en>;
			interrupt-parent = <&gpa1>;
			interrupts = <4 0 0>;
			taos,irq_gpio = <&gpa1 4 0x00>;
			taos,vled_ldo_pin = <&gpd3 3 0x00>;
			taos,prox_thd_det_hi = <55>;
			taos,prox_thd_still_det_low = <40>;
			taos,prox_thd_still_det_hi = <250>;
			taos,prox_thd_rel_low = <130>;
			taos,coef_r = <(-830)>;
			taos,coef_g = <1100>;
			taos,coef_b = <(-1180)>;
			taos,coef_c = <1000>;
			taos,dgf = <925>;
			taos,cct_coef = <3521>;
			taos,cct_offset = <2095>;
		};
	};

	hsi2c@10510000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;
		samsung,hsi2c-batcher;
		#address-cells = <1>;
		#size-cells = <0>;

		s2mpu05_pmic@66 {
			compatible = "samsung,s2mpu05-pmic";
			reg = <0x66>;
			ten-bit-address;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			g3d_en = <1>; /* 1 : enable , 0 : disable */
			buck2en_pin = "etc1-3";
			buck2en_addr = "139B0080.pinctrl";
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;   /* 1000ms */
			smpl_timer_val = <4>;   /* 500ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2017 12:00:00(Sun) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <117>;
			init_time,wday = <0>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "BUCK1";
					regulator-min-microvolt = <500000>;
					regulator-max-microvolt = <1300000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
					regulator-expected-consumer = <2>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "BUCK2";
					regulator-min-microvolt = <500000>;
					regulator-max-microvolt = <1300000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <2>;		/* MIF mode */
					regulator-expected-consumer = <4>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "BUCK3";
					regulator-min-microvolt = <500000>;
					regulator-max-microvolt = <1300000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <2>;
				};

				buck4_reg: BUCK4 {
					regulator-name = "BUCK4";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <2>;
				};

				buck5_reg: BUCK5 {
					regulator-name = "BUCK5";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <2100000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <2>;
				};

				ldo1_reg: LDO1 {
					regulator-name = "vdd_ldo1";
					regulator-min-microvolt = <650000>;
					regulator-max-microvolt = <1350000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo2_reg: LDO2 {
					regulator-name = "vqmmc";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <2800000>;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <0>;
				};

				ldo3_reg: LDO3 {
					regulator-name = "vdd_ldo3";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo4_reg: LDO4 {
					regulator-name = "vdd_ldo4";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1350000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo5_reg: LDO5 {
					regulator-name = "vdd_ldo5";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1350000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo6_reg: LDO6 {
					regulator-name = "vdd_ldo6";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1350000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo7_reg: LDO7 {
					regulator-name = "vdd_ldo7";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo8_reg: LDO8 {
					regulator-name = "vdd_ldo8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo9_reg: LDO9 {
					regulator-name = "vdd_ldo9";
					regulator-min-microvolt = <650000>;
					regulator-max-microvolt = <1350000>;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo10_reg: LDO10 {
					regulator-name = "vdd_ldo10";
					regulator-min-microvolt = <650000>;
					regulator-max-microvolt = <1350000>;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo25_reg: LDO25 {
					regulator-name = "vdd_ldo25";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo26_reg: LDO26 {
					regulator-name = "vemmc";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo27_reg: LDO27 {
					regulator-name = "vqemmc";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo29_reg: LDO29 {
					regulator-name = "GRIP_2P8";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo30_reg: LDO30 {
					regulator-name = "LCD_1P8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-initial-mode = <3>;
				};

				ldo31_reg: LDO31 {
					regulator-name = "vdd_ldo31";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo32_reg: LDO32 {
					regulator-name = "tsp_dvdd";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

				ldo33_reg: LDO33 {
					regulator-name = "VDD_CAM_AF_2P8";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
					regulator-initial-mode = <0>;
				};

				ldo34_reg: LDO34 {
					regulator-name = "VDDIO_1.8V_CAM";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-initial-mode = <0>;
				};

				ldo35_reg: LDO35 {
					regulator-name = "VDD_CAM_SENSOR_A2P8";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
					regulator-initial-mode = <0>;
				};
			};
		};

		audio_codec_cod3026x: cod3026x@04 {
			compatible = "codec,cod3026x";
			reg = <0x04>;
			ten-bit-address;
			vdd-supply = <&ldo25_reg>;
			pinctrl-names = "default";
			pinctrl-0 = <&cod3026_irq>;
			interrupt-parent = <&gpa2>;
			gpios = <&gpa2 7 0xf>;
			mic-bias1-voltage = <1>;
			mic-bias2-voltage = <1>;
			mic-bias-ldo-voltage = <3>;
			use-det-adc-mode;
			use-det-gdet-adc-mode = <1>;
			io-channels = <&exynos_adc 2>,<&exynos_adc 4>;
			io-channel-names = "adc-ear","adc-gdet";
			#io-channel-cells = <2>;
			io-channel-ranges;
			but-zones-list = <&but_zones 226 0 206>, /* send/end */
					<&but_zones 582 207 290>, /* google */
					<&but_zones 115 291 492>, /* vol up */
					<&but_zones 114 493 1300>; /* vol down */
			mic-adc-range = <1367>;
			mic-det-delay = <550>;
			water-threshold-max = <2250>;
			water-threshold-min1 = <550>;
			water-threshold-min2 = <550>;
		};
	};

	i2c@13880000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		samsung,i2c-bus-num = <20>;
		pinctrl-names = "default";
		pinctrl-0 = <&spk_amp_i2c>;
	};

	i2c@13890000{
		sec-nfc@27 {
			/delete-property/ sec-nfc,pvdd_en;
			/delete-property/ pinctrl-0;		
			pinctrl-0 = <&nfc_en &nfc_clk_req &nfc_firm>;
		};
	};
	
	/* Gtactive2lte model needs SD Card Detect Pin Setting */
	dwmmc2@13560000 {
		skip-init-mmc-scan;
		card-detect = <&gpa0 1 0xf>;
		card-detect-gpio;
		sec-sd-slot-type = <1>; /* Normal SD slot */
	};

	wlan {
		compatible = "qcom,cnss_sdio";
		wlan-en-gpio = <&gpd3 6 0x1>; /* WLAN_EN */
		wlan-host-wake = <&gpa2 2 0>; /* WLAN_HOST_WAKE */
		vdd-wlan-supply = <&cnss_dcdc_en>; /* DCDC_EN regulator */
		pinctrl-names = "default";
		pinctrl-0 = <&cnss_wlanen &cfg_wlanhostwake>;
		status = "okay";
	};

	pinctrl@13750000 {
		sd0_rdqs: sd0-rdqs {
			samsung,pins = "gpr0-2";
			samsung,pin-pud = <1>;
		};

		sd1_clk: sd1-clk {
			samsung,pin-drv = <1>;
		};

		sd1_cmd: sd1-cmd {
			samsung,pin-drv = <1>;
		};
	
		sd1_bus1: sd1-bus-width1 {
			samsung,pin-pud = <3>;
			samsung,pin-drv = <1>;
		};

		sd1_bus4: sd1-bus-width4 {
			samsung,pin-pud = <3>;
			samsung,pin-drv = <1>;
		};
	};
	
	bluetooth {
		compatible = "qca,qca6174";
		gpios = <&gpd4 0 0x1	/*BT_EN*/
				&gpd4 5 0x1	/*BT_WAKE*/
				&gpa1 6 0xF	/*BT_HOST_WAKE*/ >;
		vdd-wlan-supply = <&cnss_dcdc_en>; /* DCDC_EN regulator */
		pinctrl-names = "default";
		pinctrl-0=<&bt_hostwake &bt_btwake &bt_en>;
		status = "okay";
	};

	s1402x: s1402x@14880000 {
		pinctrl-names = "default", "idle", "bt", "bt-idle";
		pinctrl-0 = <&i2s_pmic_amp_bus
			&i2s_amp_bus>;
		pinctrl-1 = <&i2s_pmic_amp_bus_idle
			&i2s_bt_bus_idle> ;
		pinctrl-2 = <&i2s_pmic_bus
			&i2s_bt_bus>;
		pinctrl-3 = <&i2s_bt_bus_idle>;

		/delete-property/ bck-mcko-mode;
		status = "okay";
	};

	pinctrl@139B0000 {
		spk_amp_i2c: spk-amp-i2c{
			samsung,pins ="gpc1-2", "gpc1-3";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		sensor_i2c: sensor_i2c {
			samsung,pins = "gpc4-3", "gpc4-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
		};

		grip_i2c: grip_i2c {
			samsung,pins = "gpc9-1", "gpc9-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
		};
		bt_en: bt-en {
			samsung,pins ="gpd4-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};

		bt_btwake: bt-btwake {
			samsung,pins = "gpd4-5";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-pud-pdn = <0>;
		};
		sensor_ldo_en: sensor_ldo_en {
			samsung,pins = "gpf4-2";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-val = <1>;
		};

		proxy_led_en: proxy_led_en {
			samsung,pins = "gpd3-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
		};
	};

	tsp_avdd {
		compatible = "regulator-fixed";
		regulator-name = "tsp_avdd";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpd4 3 0>;
		enable-active-high;
	};

	pinctrl@139B0000 {
		tsp_en: tsp-en {
			samsung,pins = "gpd4-3";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	i2c@13840000 {
		pinctrl-names = "default";
		pinctrl-0 = <&i2c3_bus &tsp_en>;
	};
};

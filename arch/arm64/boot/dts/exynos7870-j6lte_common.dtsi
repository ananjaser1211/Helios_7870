			fnum = <2>; /* F1_9 : fimc-is-param.h */
			setfile = "setB";
		};
	};

	hsi2c_4: hsi2c@138B0000 {
		gpios = <&gpf1 0 0 &gpf1 1 0>;
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;

		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c4_bus>;


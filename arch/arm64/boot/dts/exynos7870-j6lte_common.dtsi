		fimc-is-actuator@0C {
			compatible = "samsung,exynos5-fimc-is-actuator-dw9807";
			reg = <0x0C>;
			id = <0>; /* matching sensor id */
		};
	};

	hsi2c_5: hsi2c@138C0000 {
		gpios = <&gpf0 2 0 &gpf0 3 0>;
		status = "okay";
		clock-frequency = <400000>;

		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c5_bus>;

		fimc-is-4h5yc@10 {
			compatible = "samsung,exynos5-fimc-is-cis-4h5yc";
			reg = <0x36>;
			id = <1>; /* matching sensor id */
			fnum = <2>; /* F1_9 : fimc-is-param.h */
			setfile = "setB"; /*EVT 01*/
		};
	};

	pinctrl@139B0000 {
		nfc_det: nfc-det {
			samsung,pins = "gpc0-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
		};

		nfc_pvdd_en: nfc-pvdd-en {
			samsung,pins = "gpd2-1";

		clock-frequency = <400000>;

		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c3_bus>;

		fimc-is-3l2@2D {
			compatible = "samsung,exynos5-fimc-is-cis-3l2";
			reg = <0x2D>;

			net_boost,node="rndis0";
			net_boost,table_size = <1>;
			net_boost,table= <  1 0 0 0 0 0 0 0 1 >;
		};
		boot_device@4 {
			net_boost,label="CLAT";
			net_boost,node="clat clat4 v4-rmnet0 v4-rmnet1 v4-rmnet2 v4-rmnet3 v4-rmnet4 v4-rmnet5 v4-rmnet6 v4-rmnet7";
			net_boost,table_size = <3>;
			net_boost,table= <
				120 1352000 0 0 0 902000 0 0 0
				130 1482000 0 0 0 902000 0 0 0
				150 1586000 0 0 0 902000 0 0 0
				>;
		};
		boot_device@5 {
			net_boost,label="WIFI";
			net_boost,node="wlan0";
			net_boost,table_size = <2>;
			net_boost,table= <
				50 0 0 0 0 0 0 0 0
				150 0 0 0 0 0 0 0 0
				>;
		};
		boot_device@6 {
			net_boost,label="WIFI TX";
			net_boost,node="wlan0";
			net_boost,table_size = <2>;
			net_boost,table= <
				20 1000000 0 800000 0 0 200000 0 0

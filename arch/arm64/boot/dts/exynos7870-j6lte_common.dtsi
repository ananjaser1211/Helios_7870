	};

	fimc_is_module_3l2: fimc-is_sensor_3l2@2D {
		compatible = "samsung,sensor-module-3l2";

		pinctrl-names = "default", "pin0", "pin1";
		pinctrl-0 = <&fimc_is_mclk0_default>; /*need to config for drv strength*/
		pinctrl-1 = <&fimc_is_mclk0_in>;
		pinctrl-2 = <&fimc_is_mclk0_fn>;

		position = <0>; /* Rear:0. Front:1 */
		id = <0>; /* sensor node id */
		mclk_ch = <0>;
		sensor_i2c_ch = <0>; /* I2C 0 */
		sensor_i2c_addr = <0x5A>;

		gpio_reset = <&gpd4 2 0x1>; /* sensor reset */
		gpio_camio_1p8_en = <&gpd4 3 0x1>; /* VDD_CAM_IO_1P8_EN */
		gpio_cam_1p2_en = <&gpf4 1 0x1>; /* CAM_CORE_EN */
		gpio_camaf_2p8_en = <&gpd1 1 0x1>; /* RCAM_AF_2P8_EN */
		gpio_cam_2p8_en = <&gpd3 5 0x1>; /* CAM_MAIN_2P8_EN */
		status = "okay";

		af {
			product_name = <13>; /* ACTUATOR_NAME_DW9807 */
			i2c_addr = <0x18>;
			i2c_ch = <1>;
		};

		flash {
			product_name = <11>; /* FLASH_GPIO */
			flash_first_gpio = <2>;
			flash_second_gpio = <1>;
		};
	};

	fimc_is_module_4h5yc: fimc-is_sensor_4h5yc@10 {
		compatible = "samsung,sensor-module-4h5yc";

		pinctrl-names = "default", "pin0", "pin1";
		pinctrl-0 = <&fimc_is_mclk1_default>; /*need to config for drv strength*/
		pinctrl-1 = <&fimc_is_mclk1_in>;
		pinctrl-2 = <&fimc_is_mclk1_fn>;

		position = <1>; /* Rear:0. Front:1 */
		id = <1>; /* sensor node id */
		mclk_ch = <1>;
		sensor_i2c_ch = <1>;
		sensor_i2c_addr = <0x36>;

		gpio_reset = <&gpa1 5 0x1>; /* sensor reset */
		gpio_vtcam_1p2_en = <&gpc8 0 0x1>; /* VTCAM_CORE_EN */
		gpio_camio_1p8_en = <&gpd4 3 0x1>; /* VDD_CAM_IO_1P8_EN */
		gpio_cam_2p8_en = <&gpd1 2 0x1>; /* VTCAM_A2P8_EN */
		status = "okay";
	};

	fimc_is_flash_gpio: fimc-is-flash_gpio@0 {
		compatible = "samsung,sensor-flash-gpio";
